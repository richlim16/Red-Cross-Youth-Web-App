      <h1>Uniform Request Form</h1>\r\n            </div>\r\n            <form class=\"needs-validation\" novalidate=\"\" action=\"/act/addUnifReq\" method=\"post\">\r\n                <div class=\"row\">\r\n                    <div class=\"item\">\r\n                        <label for=\"dateReceived\">Date Requested:</label>\r\n                        <input type=\"date\" class=\"form-control\" name=\"dateReceived\" placeholder=\"\" value=\"\" required=\"\">\r\n                    </div>\r\n                </div>\r\n                <div class=\"row\">\r\n                    <div class=\"item\" id=\"widerInput\">\r\n                        <label for=\"volunteer\">Name of Requesting Volunteer</label>\r\n                        <input type=\"text\" name=\"volunteer\" placeholder=\"\" value=\"\" required=\"\">\r\n                    </div>\r\n                    <div class=\"item\" id=\"wideInput\">\r\n                        <label for=\"activity\">Name of Council</label>\r\n                        <select type=\"text\" name=\"council\" required>\r\n                            <option value=\"#id\">Council of Red Crosses</option>\r\n                        </select>\r\n                    </div>\r\n                </div>\r\n                <div class=\"row\">\r\n                    <div class=\"item\">\r\n                        <label for=\"rcys\">Type</label>\r\n                        <select name=\"type\">\r\n                            <option value=\"A\">A</option>\r\n                            <option value=\"C\">C</option>\r\n                        </select>\r\n                    </div>\r\n                    <div class=\"item\">\r\n                        <label for=\"rcys\">Quantity</label>\r\n                        <input type=\"number\" class=\"form-control\" id=\"qty\" name=\"qty\" required>    \r\n                    </div>\r\n                    <div class=\"item\">\r\n                        <label for=\"size\">Size</label>\r\n                        <select class=\"form-control\" name=\"size\">\r\n                            <option value=\"XS\">XS</option>\r\n                            <option value=\"S\">S</option>\r\n                            <option value=\"M\">M</option>\r\n                            <option value=\"L\">L</option>\r\n                            <option value=\"XL\">XL</option>\r\n                        </select>     \r\n                    </div>\r\n                    <div class=\"item\">\r\n                        <label for=\"rcys\">RCY</label>\r\n                        <input type=\"radio\" name=\"design\" placeholder=\"\" value=\"0\" required>    \r\n                    </div>\r\n                    <div class=\"item\">\r\n                        <label for=\"rcys\">Advisor</label>\r\n                        <input type=\"radio\" name=\"design\" placeholder=\"\" value=\"0\" required>\r\n                    </div>\r\n                </div>\r\n                <hr>\r\n                <button type=\"submit\">Post</button>\r\n            </form>\r\n        </div>\r\n        <f/>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport navbar from '../components/Navbar.vue'\r\nimport footer from '../components/footer.vue'\r\n\r\nexport default{\r\n    name: 'uniformRequestForm',\r\n    components: {\r\n        'n':navbar,\r\n        'f': footer\r\n    }\r\n\r\n}\r\n</script>\r\n<style lang=\"css\" scoped>\r\n.breadcrumbs {\r\n    padding: 20px 0;\r\n    background-color: #f1f6fe;\r\n    min-height: 40px;\r\n    margin-top: 134px;\r\n    }\r\n\r\n    @media (max-width: 992px) {\r\n    .breadcrumbs {\r\n        margin-top: 58px;\r\n    }\r\n    }\r\n\r\n    .breadcrumbs h2 {\r\n    font-size: 24px;\r\n    font-weight: 300;\r\n    margin: 0;\r\n    }\r\n\r\n    @media (max-width: 992px) {\r\n    .breadcrumbs h2 {\r\n        margin: 0 0 10px 0;\r\n    }\r\n    }\r\n\r\n    .breadcrumbs ol {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    list-style: none;\r\n    padding: 0;\r\n    margin: 0;\r\n    font-size: 14px;\r\n    }\r\n\r\n    .breadcrumbs ol li + li {\r\n    padding-left: 10px;\r\n    }\r\n\r\n    .breadcrumbs ol li + li::before {\r\n    display: inline-block;\r\n    padding-right: 10px;\r\n    color: #6c757d;\r\n    content: \"/\";\r\n    }\r\n\r\n    .mainForm{\r\n      margin-top: 50px;\r\n      margin-bottom: 50px;\r\n      padding: 30px;\r\n      background: #f9f9f9;\r\n      box-shadow: 0 20px 20px #aaa;\r\n    }\r\n    .mainForm .title{\r\n        margin-bottom: 50px;\r\n    }\r\n    select {\r\n    -webkit-appearance: none;\r\n    -moz-appearance: none;\r\n    text-indent: 1px;\r\n    text-overflow: '';\r\n\r\n    margin: 1vh;\r\n    width: 150px;\r\n    padding: .5vh;\r\n    border: 1px solid #aaa;\r\n    color: #333;\r\n    border-radius: 3px;\r\n    transition: .4s;\r\n    }\r\n    select:focus{\r\n        border: 1px solid #00aaff;\r\n        box-shadow: 0 0 5px #00aaff;\r\n    }\r\n    .item{\r\n        display: flex;\r\n        flex-direction: column;\r\n        margin-right: 25px;\r\n        margin-bottom: 15px;\r\n    }\r\n    .item input, .item select{\r\n        width: 100%;\r\n        height: 4vh;\r\n        padding: 5px;\r\n        margin: 0;\r\n        border: 0;\r\n        border-radius: 3px;\r\n        transition: .4s;\r\n        box-shadow: 0 3px 3px -1px rgba(0, 0, 0, .5);\r\n    }\r\n    .item input:focus, .item select:focus{\r\n      outline:none;\r\n      box-shadow: 0 5px 3px -1px rgba(200, 0, 0, .8);\r\n    }\r\n    #widerInput{\r\n      width: 50%;\r\n    }\r\n    #wideInput{\r\n      width: 30%;\r\n    }\r\n    .row{\r\n        padding-left: 30px;\r\n    }\r\n    hr{\r\n        background: #e00;\r\n        height: 5px;\r\n        border-radius: 5px;\r\n        margin-bottom: 20px;\r\n    }\r\n    button{\r\n        font-size: 2vh;\r\n        padding: 1vh 2vh 1vh 2vh;\r\n        border: 0;\r\n        border-radius: 5px;\r\n        background: #e00;\r\n        color: #eee;\r\n        box-shadow: 0 5px 5px rgba(0, 0, 0, .5);\r\n        transition: .2s;\r\n    }\r\n    button:hover{\r\n        background: #f00;\r\n        margin-top: 5px;\r\n        box-shadow: none;\r\n    }\r\n    @media (max-width: 768px) {\r\n    .breadcrumbs .d-flex {\r\n        display: block !important;\r\n    }\r\n    .breadcrumbs ol {\r\n        display: block;\r\n    }\r\n    .breadcrumbs ol li {\r\n        display: inline-block;\r\n    }\r\n    .row{\r\n      flex-direction: column;\r\n    }\r\n    .item, #widerInput, #wideInput, button{\r\n      width: 100%;\r\n    }\r\n\r\n    }\r\n\r\n    /* end of @media */\r\n</style>\r\n"]}]}